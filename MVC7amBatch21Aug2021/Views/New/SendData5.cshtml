@using MVC7amBatch21Aug2021.Models

@model List<EmployeeModel> 
@{
    ViewBag.Title = "SendData5";
}

<h2>SendData5</h2>

<table class="table table-bordered table-responsive">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table>

